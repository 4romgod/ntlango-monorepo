{
  "name": "ntlango-cdk",
  "description": "AWS CDK Infrastructure for Dyondo muliti-user blogging platform",
  "version": "0.1.0",
  "license": "UNLICENSED",
  "main": "dist/lib/app.js",
  "scripts": {
    "build": "npm run clean && npm run lint && tsc",
    "clean": "rm -rf dist && rm -rf cdk.out",
    "watch": "tsc -w",
    "test": "jest --verbose",
    "cdk": "cdk",
    "lint": "npm run prettier-format && eslint lib/* --ext .ts --fix",
    "prettier-format": "npm run prettier-format-lib && npm run prettier-format-test",
    "prettier-format-lib": "prettier --no-error-on-unmatched-pattern --config .prettierrc.json 'lib/**/*.{js,ts}' --write",
    "prettier-format-test": "prettier --no-error-on-unmatched-pattern --config .prettierrc.json 'test/**/*.{js,ts}' --write"
  },
  "devDependencies": {
    "@types/jest": "^26.0.10",
    "@types/node": "10.17.27",
    "@types/prettier": "2.6.0",
    "@typescript-eslint/eslint-plugin": "^5.30.4",
    "@typescript-eslint/parser": "^5.30.4",
    "aws-cdk": "2.7.0",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^26.4.2",
    "ts-jest": "^26.2.0",
    "ts-node": "^9.0.0",
    "typescript": "~3.9.7"
  },
  "dependencies": {
    "aws-cdk-lib": "2.7.0",
    "constructs": "^10.0.0"
  }
}
