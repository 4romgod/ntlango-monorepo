{
  "name": "@gatherle/webapp",
  "private": true,
  "scripts": {
    "build": "npm-run-all codegen lint:check build:ts build:next",
    "build:ts": "echo '\nTranspilling ts source code...' && tsc --build --pretty --verbose && echo 'All done with the build!'",
    "build:next": "NEXT_SKIP_ESLINT=1 next build -d",
    "clean:types": "rm -rf .next",
    "codegen": "graphql-codegen",
    "codegen:watch": "graphql-codegen --watch",
    "dev": "npm-run-all codegen start:dev",
    "lint:check": "eslint .",
    "lint:fix": "eslint . --fix",
    "start:dev": "next dev",
    "start:prod": "next start",
    "test:unit": "jest --config=test/unit/jest.config.ts --runInBand",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ui": "playwright test --ui",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@apollo/client": "^3.13.8",
    "@apollo/client-integration-nextjs": "^0.12.2",
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "@heroicons/react": "^2.0.18",
    "@mui/icons-material": "^7.1.0",
    "@mui/material": "^7.1.0",
    "@mui/material-nextjs": "^7.1.0",
    "@mui/x-date-pickers": "^7.1.0",
    "@tailwindcss/forms": "^0.5.7",
    "autoprefixer": "10.4.15",
    "clsx": "^2.0.0",
    "date-fns": "^3.6.0",
    "dayjs": "^1.11.12",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "next": "^16.1.6",
    "next-auth": "^5.0.0-beta.19",
    "nprogress": "^0.2.0",
    "postcss": "8.4.31",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.2.1",
    "rrule": "^2.8.1",
    "tailwindcss": "3.3.3",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.6",
    "@graphql-codegen/client-preset": "^4.2.5",
    "@parcel/watcher": "^2.4.1",
    "@playwright/test": "^1.58.2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/bcrypt": "^5.0.1",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/lodash": "^4.14.195",
    "@types/nprogress": "^0.2.3",
    "@types/react": "^19.1.4",
    "@types/react-dom": "^19.1.5",
    "@vercel/style-guide": "^0.1.0",
    "depcheck": "^1.4.7",
    "eslint": "^9.0.0",
    "eslint-config-next": "^15.0.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-unused-imports": "^4.3.0",
    "graphql": "^16.11.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.5.4"
  },
  "engines": {
    "node": ">=22.x"
  }
}
