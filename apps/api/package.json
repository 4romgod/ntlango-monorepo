{
  "name": "@ntlango/backend",
  "version": "1.0.0",
  "description": "This repository contains the business logic for the backend",
  "main": "dist/lib/server.js",
  "types": "dist/types/lib/server.d.ts",
  "scripts": {
    "build": "tsc --pretty",
    "dev": "nodemon ./lib/server.ts",
    "start": "npm run build && node --trace-warnings dist/lib/server.js",
    "format": "prettier --no-error-on-unmatched-pattern --config .prettierrc.json 'lib/**/*.{js,ts}' 'test/**/*.{js,ts}' --write",
    "seed": "npm run build && node -r dotenv/config ./dist/scripts/seed.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@apollo/server": "^4.10.4",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "graphql": "^16.8.1",
    "graphql-type-json": "^0.3.2",
    "lodash": "^4.17.21",
    "mongoose": "^8.3.0",
    "morgan": "^1.10.0",
    "reflect-metadata": "^0.2.2",
    "type-graphql": "^2.0.0-rc.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/sinon": "^17.0.3",
    "@types/uuid": "^9.0.8",
    "dotenv": "^16.4.5",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.4"
  }
}
